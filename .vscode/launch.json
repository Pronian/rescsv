{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Deno Create CSV",
      "type": "node",
      "request": "launch",
      "cwd": "${workspaceFolder}",
      "runtimeExecutable": "deno",
      "runtimeArgs": [
        "run",
        "--unstable",
        "--import-map=import_map.json",
        "--inspect-brk",
        "-A",
        "mod.ts",
        "-c",
        "account"
      ],
      "outputCapture": "std",
      "port": 9229
    },
    {
      "name": "Deno Save To Props",
      "type": "node",
      "request": "launch",
      "cwd": "${workspaceFolder}",
      "runtimeExecutable": "deno",
      "runtimeArgs": [
        "run",
        "--unstable",
        "--import-map=import_map.json",
        "--inspect-brk",
        "-A",
        "mod.ts",
        "-s",
        "test",
        "-d"
      ],
      "outputCapture": "std",
      "port": 9229
    },
    {
      "name": "Deno Test",
      "type": "node",
      "request": "launch",
      "cwd": "${workspaceFolder}",
      "runtimeExecutable": "deno",
      "runtimeArgs": [
        "test",
        "--unstable",
        "--import-map=import_map.json",
        "--inspect-brk",
        "-A"
      ],
      "outputCapture": "std",
      "port": 9229
    }
  ]
}
